{
  "name": "sparqljson-to-tree",
  "version": "2.0.0",
  "description": "Converts SPARQL JSON results to a tree-based structure",
  "keywords": [
    "graphql",
    "sparql",
    "query",
    "rdf",
    "linked data"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/sparqljson-to-tree.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/sparqljson-to-tree.js/issues"
  },
  "homepage": "https://github.com/rubensworks/sparqljson-to-tree.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@rdfjs/data-model": "^1.1.1",
    "@types/jest": "^25.1.0",
    "@types/minimist": "^1.2.0",
    "coveralls": "^3.0.0",
    "jest": "^25.0.0",
    "manual-git-changelog": "^1.0.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^25.1.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "rdf-literal": "^1.0.0",
    "sparqljson-parse": "^1.5.0"
  }
}
